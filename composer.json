{
    "name": "boolnut/boolnut",
    "type": "project",
    "description": "The Boolnut Framework.",
    "keywords": [
      "framework",
      "boolnut"
    ],
    "license": "MIT",
    "autoload": {
      "classmap": [
        "./"
      ]
    },

    "require": {
        "php": "^7.4",
        "ext-pdo": "*",
        "php-curl-class/php-curl-class": "^9.5",
        "symfony/console": "^2.6.7",
        "twig/twig": "^3.0"
    },
    "scripts": {
      "post-root-package-install": [
        "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ]
    },
    "extra": {
      "miyaave": {
        "dont-discover": []
      }
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
